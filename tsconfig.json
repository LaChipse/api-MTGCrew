{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2015"]
  },
  "include": [
    "api/**/*.ts",
    "src/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
    